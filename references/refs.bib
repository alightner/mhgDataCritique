	keywords = {Connectionism, Neural computation, Classicism, Cognitivism, Computation, Computational neuroscience, Computational theory of mind, Computationalism, Information processing, Meaning, Representation},
	pages = {1--38},
	file = {Full Text:C\:\\Users\\au687269\\Zotero\\storage\\6D3RR9T5\\Piccinini and Scarantino - 2011 - Information processing, computation, and cognition.pdf:application/pdf},
}

