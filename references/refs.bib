	pages = {20180180},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\MF5V3VRA\\Barclay Pat et al. - 2018 - State-dependent risk-taking.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\J9THZ34T\\rspb.2018.html:text/html},
}

