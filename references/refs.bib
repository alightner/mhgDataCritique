	keywords = {Loss attention, Loss aversion, MouselabWEB},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\JYTX4YUR\\Lejarraga et al. - 2019 - The attention–aversion gap how allocation of atte.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\I7ZQSJCA\\S1090513818302769.html:text/html},
}

