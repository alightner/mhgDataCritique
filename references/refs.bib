	keywords = {life history, social status, Biology, Life Sciences, aggression network, Integrative Biology, rank changes, social instability, transitivity},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\2YBCF59W\\Strauss and Shizuka - 2021 - The dynamics of dominance open questions, challen.pdf:application/pdf},
}

