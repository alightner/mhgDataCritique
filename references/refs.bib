@article{pisorSupplementaryMaterialPreferences2020,
	title = {Supplementary material from "{Preferences} and constraints: the value of economic games for studying human behaviour"},
	copyright = {CC BY 4.0},
	shorttitle = {Supplementary material from "{Preferences} and constraints},
	url = {https://rs.figshare.com/collections/Supplementary_material_from_Preferences_and_constraints_the_value_of_economic_games_for_studying_human_behaviour_/4994576},
	doi = {10.6084/M9.FIGSHARE.C.4994576},
	abstract = {As economic games have spread from experimental economics to other social sciences, so too have critiques of their usefulness for drawing inferences about the ‘real world’. What these criticisms often miss is that games can be used to reveal individuals' private preferences in ways that observational and interview data cannot; furthermore, economic games can be designed such that they \textit{do} provide insights into real-world behaviour. Here, we draw on our collective experience using economic games in field contexts to illustrate how researchers can strategically alter the framing or design of economic games to draw inferences about private-world or real-world preferences. A detailed case study from coastal Colombia provides an example of the subtleties of game design and how games can be combined fruitfully with self-report data. We close with a list of concrete recommendations for how to modify economic games to better match particular research questions and research contexts.},
	urldate = {2020-11-23},
	author = {Pisor, Anne C. and Gervais, Matthew M. and Purzycki, Benjamin G. and Ross, Cody T.},
	year = {2020},
	note = {Publisher: The Royal Society},
	keywords = {170299 Cognitive Science not elsewhere classified, 179999 Psychology and Cognitive Sciences not elsewhere classified, 60801 Animal Behaviour, FOS: Biological sciences, FOS: Psychology},
}

