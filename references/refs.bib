	month = feb,
	year = {2011},
	keywords = {error management theory, afterlife beliefs, cheater punishment, cognitive science of religion, evolution of religion, human cooperation, moralizing gods, supernatural punishment},
	pages = {46--99},
	file = {Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\XNP2VZ3G\\2153599X.2011.html:text/html},
}

