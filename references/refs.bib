	url = {http://www.sciencedirect.com/science/article/pii/S0304397510006122},
	doi = {10.1016/j.tcs.2010.11.001},
	abstract = {Social networks are an interesting class of graphs likely to become of increasing importance in the future, not only theoretically, but also for its probable applications to ad hoc and mobile networking. Rumor spreading is one of the basic mechanisms for information dissemination in networks; its relevance stemming from its simplicity of implementation and effectiveness. In this paper, we study the performance of rumor spreading in the classic preferential attachment model of Bollobás et al. which is considered to be a valuable model for social networks. We prove that, in these networks: (a) The standard PUSH–PULL strategy delivers the message to all nodes within O(log2n) rounds with high probability; (b) by themselves, PUSH and PULL require polynomially many rounds. (These results are under the assumption that m, the number of new links added with each new node is at least 2. If m=1 the graph is disconnected with high probability, so no rumor spreading strategy can work.) Our analysis is based on a careful study of some new properties of preferential attachment graphs which could be of independent interest.},
	number = {24},
	urldate = {2018-03-27},
	journal = {Theoretical Computer Science},
	author = {Chierichetti, Flavio and Lattanzi, Silvio and Panconesi, Alessandro},
	month = may,
	year = {2011},
	keywords = {Preferential attachment, Rumour spreading},
	pages = {2602--2610},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\WRUQCD3A\\Chierichetti et al. - 2011 - Rumor spreading in social networks.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\YMXZB66D\\S0304397510006122.html:text/html},
}

@article{aktipisathenaPrinciplesCooperationSystems2015,
	title = {Principles of cooperation across systems: from human sharing to multicellularity and cancer},
	volume = {9},
	issn = {1752-4571},
	shorttitle = {Principles of cooperation across systems},
	url = {https://onlinelibrary.wiley.com/doi/full/10.1111/eva.12303},
	doi = {10.1111/eva.12303},
	abstract = {Abstract From cells to societies, several general principles arise again and again that facilitate cooperation and suppress conflict. In this study, I describe three general principles of cooperation and how they operate across systems including human sharing, cooperation in animal and insect societies and the massively large?scale cooperation that occurs in our multicellular bodies. The first principle is that of Walk Away: that cooperation is enhanced when individuals can leave uncooperative partners. The second principle is that resource sharing is often based on the need of the recipient (i.e., need?based transfers) rather than on strict account?keeping. And the last principle is that effective scaling up of cooperation requires increasingly sophisticated and costly cheater suppression mechanisms. By comparing how these principles operate across systems, we can better understand the constraints on cooperation. This can facilitate the discovery of novel ways to enhance cooperation and suppress cheating in its many forms, from social exploitation to cancer.},
	number = {1},
	urldate = {2018-03-27},
	journal = {Evolutionary Applications},
	author = {{Aktipis Athena}},
	month = oct,
	year = {2015},
	keywords = {cancer suppression, cheater suppression, food sharing, kinship, need‐based transfers, Walk Away},
	pages = {17--36},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\CL7UQ2NA\\Aktipis Athena - 2015 - Principles of cooperation across systems from hum.pdf:application/pdf;Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\NMZXC3AA\\Aktipis Athena - 2015 - Principles of cooperation across systems from hum.pdf:application/pdf;Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\TKHIZSXE\\Aktipis Athena - 2015 - Principles of cooperation across systems from hum.pdf:application/pdf;Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\B4399IK7\\Aktipis Athena - 2015 - Principles of cooperation across systems from hum.pdf:application/pdf},
