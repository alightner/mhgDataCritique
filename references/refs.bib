@article{kouvarisHowEvolutionLearns2017,
	title = {How evolution learns to generalise: {Using} the principles of learning theory to understand the evolution of developmental organisation},
	volume = {13},
	issn = {1553-7358},
	shorttitle = {How evolution learns to generalise},
	url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1005358},
	doi = {10.1371/journal.pcbi.1005358},
	abstract = {Author summary A striking feature of evolving organisms is their ability to acquire novel characteristics that help them adapt in new environments. The origin and the conditions of such ability remain elusive and is a long-standing question in evolutionary biology. Recent theory suggests that organisms can evolve designs that help them generate novel features that are more likely to be beneficial. Specifically, this is possible when the environments that organisms are exposed to share common regularities. However, the organisms develop robust designs that tend to produce what had been selected in the past and might be inflexible for future environments. The resolution comes from a recent theory introduced by Watson and Szathmáry that suggests a deep analogy between learning and evolution. Accordingly, here we utilise learning theory to explain the conditions that lead to more evolvable designs. We successfully demonstrate this by equating evolvability to the way humans and machines generalise to previously-unseen situations. Specifically, we show that the same conditions that enhance generalisation in learning systems have biological analogues and help us understand why environmental noise and the reproductive and maintenance costs of gene-regulatory connections can lead to more evolvable designs.},
	language = {en},
	number = {4},
	urldate = {2018-02-12},
	journal = {PLOS Computational Biology},
	author = {Kouvaris, Kostas and Clune, Jeff and Kounios, Loizos and Brede, Markus and Watson, Richard A.},
	month = apr,
	year = {2017},
	keywords = {Natural selection, Learning, Phenotypes, Evolutionary developmental biology, Sound pressure, Organismal evolution, Evolutionary genetics, Evolutionary adaptation},
	pages = {e1005358},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\G86QN3PZ\\Kouvaris et al. - 2017 - How evolution learns to generalise Using the prin.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\85268USZ\\article.html:text/html},
}

@article{watsonHowCanEvolution2016,
	title = {How {Can} {Evolution} {Learn}?},
	volume = {31},
	issn = {0169-5347},
	url = {http://www.sciencedirect.com/science/article/pii/S0169534715002931},
	doi = {10.1016/j.tree.2015.11.009},
	abstract = {The theory of evolution links random variation and selection to incremental adaptation. In a different intellectual domain, learning theory links incremental adaptation (e.g., from positive and/or negative reinforcement) to intelligent behaviour. Specifically, learning theory explains how incremental adaptation can acquire knowledge from past experience and use it to direct future behaviours toward favourable outcomes. Until recently such cognitive learning seemed irrelevant to the ‘uninformed’ process of evolution. In our opinion, however, new results formally linking evolutionary processes to the principles of learning might provide solutions to several evolutionary puzzles – the evolution of evolvability, the evolution of ecological organisation, and evolutionary transitions in individuality. If so, the ability for evolution to learn might explain how it produces such apparently intelligent designs.},
	number = {2},
	journal = {Trends in Ecology \& Evolution},
	author = {Watson, Richard A. and Szathmáry, Eörs},
	month = feb,
	year = {2016},
	pages = {147--157},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\84MBM6X9\\Watson and Szathmáry - 2016 - How Can Evolution Learn.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\VURMDHK2\\S0169534715002931.html:text/html},
}

