	file = {Accepted Version:C\:\\Users\\au687269\\Zotero\\storage\\GZ9HSL22\\Henrich et al. - 2010 - Markets, Religion, Community Size, and the Evoluti.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\RWSXMV2C\\1480.html:text/html},
