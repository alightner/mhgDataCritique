	pages = {20142209},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\IY8K7593\\Mesoudi et al. - 2015 - Higher frequency of social learning in China than .pdf:application/pdf},
}

@article{evansWhoDoesnTrust2020,
