	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\55D47H59\\Lyon and Montgomerie - 2012 - Sexual selection is a form of social selection.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\Y6EYYQJZ\\2266.html:text/html},
