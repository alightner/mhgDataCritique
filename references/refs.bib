	journal = {Science},
	author = {Dean, L. G. and Kendal, R. L. and Schapiro, S. J. and Thierry, B. and Laland, K. N.},
	month = mar,
	year = {2012},
	pmid = {22383851},
	pages = {1114--1118},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\BM4XD9LI\\Dean et al. - 2012 - Identification of the Social and Cognitive Process.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\YDHQQCN6\\1114.html:text/html},
}

