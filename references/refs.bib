	keywords = {Cultural Moran model, Direct bias, Gumbel distribution},
	pages = {38--47},
	file = {ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\T6CHKVDY\\S0040580912000482.html:text/html},
}

