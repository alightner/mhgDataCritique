	author = {Bettencourt, Luís M. A. and Cintrón-Arias, Ariel and Kaiser, David I. and Castillo-Chávez, Carlos},
	month = may,
	year = {2006},
	keywords = {Epidemiological models, Rumor models, Scientific idea-diffusion, Transition parameter estimation},
	pages = {513--536},
	file = {ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\W4YCCDJE\\S0378437105010113.html:text/html;Submitted Version:C\:\\Users\\au687269\\Zotero\\storage\\4DS5AH58\\Bettencourt et al. - 2006 - The power of a good idea Quantitative modeling of.pdf:application/pdf},
}

@article{ahnFlavorNetworkPrinciples2011,
