@article{almaatouqAdaptiveSocialNetworks2020,
	title = {Adaptive social networks promote the wisdom of crowds},
	volume = {117},
	copyright = {Copyright © 2020 the Author(s). Published by PNAS.. https://creativecommons.org/licenses/by-nc-nd/4.0/This open access article is distributed under Creative Commons Attribution-NonCommercial-NoDerivatives License 4.0 (CC BY-NC-ND).},
	issn = {0027-8424, 1091-6490},
	url = {https://www.pnas.org/content/117/21/11379},
	doi = {10.1073/pnas.1917687117},
	abstract = {Social networks continuously change as new ties are created and existing ones fade. It is widely acknowledged that our social embedding has a substantial impact on what information we receive and how we form beliefs and make decisions. However, most empirical studies on the role of social networks in collective intelligence have overlooked the dynamic nature of social networks and its role in fostering adaptive collective intelligence. Therefore, little is known about how groups of individuals dynamically modify their local connections and, accordingly, the topology of the network of interactions to respond to changing environmental conditions. In this paper, we address this question through a series of behavioral experiments and supporting simulations. Our results reveal that, in the presence of plasticity and feedback, social networks can adapt to biased and changing information environments and produce collective estimates that are more accurate than their best-performing member. To explain these results, we explore two mechanisms: 1) a global-adaptation mechanism where the structural connectivity of the network itself changes such that it amplifies the estimates of high-performing members within the group (i.e., the network “edges” encode the computation); and 2) a local-adaptation mechanism where accurate individuals are more resistant to social influence (i.e., adjustments to the attributes of the “node” in the network); therefore, their initial belief is disproportionately weighted in the collective estimate. Our findings substantiate the role of social-network plasticity and feedback as key adaptive mechanisms for refining individual and collective judgments.},
	language = {en},
	number = {21},
	urldate = {2020-09-04},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Almaatouq, Abdullah and Noriega-Campero, Alejandro and Alotaibi, Abdulrahman and Krafft, P. M. and Moussaid, Mehdi and Pentland, Alex},
	month = may,
	year = {2020},
	pmid = {32393632},
	note = {ISBN: 9781917687119
Publisher: National Academy of Sciences
Section: Social Sciences},
	keywords = {social networks, collective intelligence, complex adaptive systems, virtual labs, wisdom of crowds},
	pages = {11379--11386},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\XIPI2UBE\\Almaatouq et al. - 2020 - Adaptive social networks promote the wisdom of cro.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\QNMLRQA2\\11379.html:text/html},
}

@article{nunnInfectiousDiseaseGroup2015,
	title = {Infectious disease and group size: more than just a numbers game},
	volume = {370},
	shorttitle = {Infectious disease and group size},
	url = {https://royalsocietypublishing.org/doi/10.1098/rstb.2014.0111},
	doi = {10.1098/rstb.2014.0111},
	abstract = {Increased risk of infectious disease is assumed to be a major cost of group living, yet empirical evidence for this effect is mixed. We studied whether larger social groups are more subdivided structurally. If so, the social subdivisions that form in larger groups may act as barriers to the spread of infection, weakening the association between group size and infectious disease. To investigate this ‘social bottleneck’ hypothesis, we examined the association between group size and four network structure metrics in 43 vertebrate and invertebrate species. We focused on metrics involving modularity, clustering, distance and centralization. In a meta-analysis of intraspecific variation in social networks, modularity showed positive associations with network size, with a weaker but still positive effect in cross-species analyses. Network distance also showed a positive association with group size when using intraspecific variation. We then used a theoretical model to explore the effects of subgrouping relative to other effects that influence disease spread in socially structured populations. Outbreaks reached higher prevalence when groups were larger, but subgrouping reduced prevalence. Subgrouping also acted as a ‘brake’ on disease spread between groups. We suggest research directions to understand the conditions under which larger groups become more subdivided, and to devise new metrics that account for subgrouping when investigating the links between sociality and infectious disease risk.},
	number = {1669},
	urldate = {2020-09-04},
	journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
	author = {Nunn, Charles L. and Jordán, Ferenc and McCabe, Collin M. and Verdolin, Jennifer L. and Fewell, Jennifer H.},
	month = may,
	year = {2015},
