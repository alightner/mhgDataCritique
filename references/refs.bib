	month = may,
	year = {2020},
	keywords = {Personality, Genetics, Individual differences, Balancing selection, Developmental plasticity, Ecological gambit, Fast-slow continuum, Life history theory, Pace-of-life syndromes, Physiology},
	file = {ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\RH2N23PY\\S109051382030060X.html:text/html},
}

