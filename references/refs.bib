	journal = {American Journal of Physical Anthropology},
	author = {Port, Markus and Hildenbrandt, Hanno and Pen, Ido and Schülke, Oliver and Ostner, Julia and Weissing, Franz J.},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ajpa.24123},
	keywords = {kin selection, social evolution, behavioral reaction norms, individual-based simulations, primate sociality, socioecological model},
	pages = {e24123},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\Y4CFEFVK\\Port et al. - The evolution of social philopatry in female prima.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\7NXF9UUV\\ajpa.html:text/html},
