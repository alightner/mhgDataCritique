@article{urosevicDysregulationBehavioralApproach2008,
	title = {Dysregulation of the behavioral approach system ({BAS}) in bipolar spectrum disorders: {Review} of theory and evidence},
	volume = {28},
	issn = {0272-7358},
	shorttitle = {Dysregulation of the behavioral approach system ({BAS}) in bipolar spectrum disorders},
	url = {http://www.sciencedirect.com/science/article/pii/S027273580800086X},
	doi = {10.1016/j.cpr.2008.04.004},
	abstract = {In recent years, a call for increased research on bipolar disorder has been answered with methodologically diverse studies exploring goal striving, life events, cognitive style, decision-making, and neurobiological abnormalities in bipolar disorder. In order to further this spurt of research and to systematize our understanding of bipolar disorder, an integrative perspective is warranted. The behavioral approach system (BAS) dysregulation theory, proposed by Richard Depue and colleagues, provides such an integrated model for understanding psychosocial and biological aspects of bipolar disorder. In this paper, we review studies on life events, cognitive style and other psychosocial and neurobiological factors to examine whether the BAS dysregulation theory is supported by existing data. Then, we draw on recent advances in the study of emotion and motivation, and propose an expansion of the BAS dysregulation model of bipolar spectrum disorders to foster further biopsychosocial investigations of bipolar disorder. This expanded model provides greater specificity in predictions, especially about the nature of BAS dysregulation, environmental factors and psychological processes (e.g., appraisal processes) featured in a causal chain culminating in bipolar symptoms. Finally, we discuss the implications of the expanded BAS model for the course of bipolar spectrum disorders.},
	number = {7},
	urldate = {2015-09-23},
	journal = {Clinical Psychology Review},
	author = {Urošević, Snežana and Abramson, Lyn Y. and Harmon-Jones, Eddie and Alloy, Lauren B.},
	month = oct,
	year = {2008},
	keywords = {Behavioral approach system (BAS), Bipolar disorder, Depression, Goal striving, Hypomania, Mania},
	pages = {1188--1205},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\TFQUWWE5\\Urošević et al. - 2008 - Dysregulation of the behavioral approach system (B.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\IT5A4Z6J\\Urošević et al. - 2008 - Dysregulation of the behavioral approach system (B.html:text/html},
