@article{mermelsteinSheToldMe2019,
	title = {“{She} told me about a singing cactus”: {The} sources of counterintuitive concepts are recalled better than those of ordinary concepts},
	shorttitle = {“{She} told me about a singing cactus”},
	url = {https://psyarxiv.com/6cp8e/},
	doi = {10.31234/osf.io/6cp8e},
	abstract = {Communication is central to human life, yet it leaves humans vulnerable to misinformation and manipulation. Here, we tested the hypothesis that mechanisms for evaluating communication strategically hold on to the source of communicated information that is inconsistent with pre-existing beliefs. In four experiments, participants read stories from different sources containing concepts that violate core knowledge intuitions (counterintuitive concepts) and ordinary concepts. Exp. 1 revealed that after a brief delay (2-min) participants more accurately attributed counterintuitive versus ordinary concepts to their source. Exp. 2a-b replicated this finding and found that this attribution accuracy advantage extended to other contextual information (places and dates). Exp. 3, using a longitudinal design, found that after a longer delay (48-hour versus 20-min) this attribution accuracy advantage for counterintuitive concepts persisted for persons but not for other contextual information. We discuss the implications of these findings to the functional properties of mechanisms designed for monitoring communication.},
	urldate = {2019-06-08},
	author = {Mermelstein, Spencer and Barlev, Michael and German, Tamsin},
	month = jun,
	year = {2019},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\FU5SKDIN\\Mermelstein et al. - 2019 - “She told me about a singing cactus” The sources .pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\CTCIQ92N\\6cp8e.html:text/html},
}

@article{tarnitaEvolutionaryDynamicsSet2009,
	title = {Evolutionary dynamics in set structured populations},
	volume = {106},
	copyright = {© 2009},
	issn = {0027-8424, 1091-6490},
	url = {https://www.pnas.org/content/106/21/8601},
	doi = {10.1073/pnas.0903019106},
	abstract = {Evolutionary dynamics are strongly affected by population structure. The outcome of an evolutionary process in a well-mixed population can be very different from that in a structured population. We introduce a powerful method to study dynamical population structure: evolutionary set theory. The individuals of a population are distributed over sets. Individuals interact with others who are in the same set. Any 2 individuals can have several sets in common. Some sets can be empty, whereas others have many members. Interactions occur in terms of an evolutionary game. The payoff of the game is interpreted as fitness. Both the strategy and the set memberships change under evolutionary updating. Therefore, the population structure itself is a consequence of evolutionary dynamics. We construct a general mathematical approach for studying any evolutionary game in set structured populations. As a particular example, we study the evolution of cooperation and derive precise conditions for cooperators to be selected over defectors.},
	language = {en},
	number = {21},
	urldate = {2019-06-11},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Tarnita, Corina E. and Antal, Tibor and Ohtsuki, Hisashi and Nowak, Martin A.},
	month = may,
	year = {2009},
