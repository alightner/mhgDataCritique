	keywords = {Cognition, Culture, morality, intentions, human universals},
	pages = {4688--4693},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\2NQ7J45X\\Barrett et al. - 2016 - Small-scale societies exhibit fundamental variatio.pdf:application/pdf;Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\CPC5MXGE\\Barrett et al. - 2016 - Small-scale societies exhibit fundamental variatio.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\W3TUQ5HF\\4688.html:text/html;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\UA5U3B3X\\4688.html:text/html},
