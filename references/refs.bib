	author = {Rouault, Marion and Fleming, Stephen M.},
	month = oct,
	year = {2020},
	pmid = {33060292},
	note = {Publisher: National Academy of Sciences
Section: Social Sciences},
	keywords = {fMRI, confidence, metacognition, self-efficacy},
