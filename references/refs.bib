@article{liebenbergMusicDanceSynchrony2017,
	title = {Music, dance, synchrony, and conformity: {Dealing} with non-cooperation in early hominin culture},
	volume = {15},
	shorttitle = {Music, dance, synchrony, and conformity},
	url = {https://akademiai.com/doi/abs/10.1556/2050.2017.0002},
	doi = {10.1556/2050.2017.0002},
	abstract = {Using Robin Dunbar’s theories about hominin social bonding and the evolution of music, dance and religion, this article argues that a proto-ritualistic collective song and dance performance established conditions that were ideal for the evolution of fundamental elements of human cognition, such as shared intentionality, joint attention, and the ability to form a mental template. Furthermore, it is argued that this dance routine would have been an effective breeding ground for the development of a psychological regulatory system that could powerfully enforce cooperation by collectively targeting individuals who do not or cannot conform to the melody or the beat – i.e. misfits who do not achieve synchrony with the other members of the dance group. Thus a hominin society consisting of highly cooperative individuals possessing the cognitive skills of shared intentionality, joint attention, and the ability to form mental templates – i.e. a society with all the necessary skills for the development of a mimetic form of communication, followed much later by spoken language – comes into being.},
	number = {1},
	urldate = {2018-09-23},
	journal = {Evolution, Mind and Behaviour},
	author = {Liebenberg, D.},
	month = dec,
	year = {2017},
	pages = {1--11},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\U27W6S8Q\\Liebenberg - 2017 - Music, dance, synchrony, and conformity Dealing w.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\ZGGCTNXW\\2050.2017.html:text/html},
}

@article{youngDisruptionRightTemporoparietal2010,
	title = {Disruption of the right temporoparietal junction with transcranial magnetic stimulation reduces the role of beliefs in moral judgments},
	volume = {107},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/107/15/6753},
	doi = {10.1073/pnas.0914826107},
	abstract = {When we judge an action as morally right or wrong, we rely on our capacity to infer the actor's mental states (e.g., beliefs, intentions). Here, we test the hypothesis that the right temporoparietal junction (RTPJ), an area involved in mental state reasoning, is necessary for making moral judgments. In two experiments, we used transcranial magnetic stimulation (TMS) to disrupt neural activity in the RTPJ transiently before moral judgment (experiment 1, offline stimulation) and during moral judgment (experiment 2, online stimulation). In both experiments, TMS to the RTPJ led participants to rely less on the actor's mental states. A particularly striking effect occurred for attempted harms (e.g., actors who intended but failed to do harm): Relative to TMS to a control site, TMS to the RTPJ caused participants to judge attempted harms as less morally forbidden and more morally permissible. Thus, interfering with activity in the RTPJ disrupts the capacity to use mental states in moral judgment, especially in the case of attempted harms.},
	language = {en},
	number = {15},
	urldate = {2018-09-23},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Young, Liane and Camprodon, Joan Albert and Hauser, Marc and Pascual-Leone, Alvaro and Saxe, Rebecca},
	month = apr,
	year = {2010},
	pmid = {20351278},
	keywords = {morality, functional MRI, theory of mind},
	pages = {6753--6758},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\QZI9GV4G\\Young et al. - 2010 - Disruption of the right temporoparietal junction w.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\HC76US69\\6753.html:text/html},
}

