	author = {Barwich, Ann-Sophie},
	year = {2019},
	keywords = {localization, Gnostic units, grandmother cells, History of Science, Localist theory, Model pluralism, object recogntion, Philosphy of science, Sparse Coding},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\HXTHIPPK\\Barwich - 2019 - The Value of Failure in Science The Story of Gran.pdf:application/pdf},
}

