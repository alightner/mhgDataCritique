@article{bettencourtPowerGoodIdea2006,
	title = {The power of a good idea: quantitative modeling of the spread of ideas from epidemiological models},
	volume = {364},
	issn = {03784371},
	shorttitle = {The power of a good idea},
	url = {http://arxiv.org/abs/physics/0502067},
	doi = {10.1016/j.physa.2005.08.083},
	abstract = {The population dynamics underlying the diffusion of ideas hold many qualitative similarities to those involved in the spread of infections. In spite of much suggestive evidence this analogy is hardly ever quantified in useful ways. The standard benefit of modeling epidemics is the ability to estimate quantitatively population average parameters, such as interpersonal contact rates, incubation times, duration of infectious periods, etc. In most cases such quantities generalize naturally to the spread of ideas and provide a simple means of quantifying sociological and behavioral patterns. Here we apply several paradigmatic models of epidemics to empirical data on the advent and spread of Feynman diagrams through the theoretical physics communities of the USA, Japan, and the USSR in the period immediately after World War II. This test case has the advantage of having been studied historically in great detail, which allows validation of our results. We estimate the effectiveness of adoption of the idea in the three communities and find values for parameters reflecting both intentional social organization and long lifetimes for the idea. These features are probably general characteristics of the spread of ideas, but not of common epidemics.},
	urldate = {2018-05-03},
	journal = {Physica A: Statistical Mechanics and its Applications},
	author = {Bettencourt, Luís M. A. and Cintrón-Arias, Ariel and Kaiser, David I. and Castillo-Chávez, Carlos},
	month = may,
	year = {2006},
	note = {arXiv: physics/0502067},
	keywords = {Physics - Physics and Society},
	pages = {513--536},
	file = {arXiv\:physics/0502067 PDF:C\:\\Users\\au687269\\Zotero\\storage\\T38BYGN3\\Bettencourt et al. - 2006 - The power of a good idea quantitative modeling of.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\P7HXKSS8\\0502067.html:text/html},
}

@article{graySuperblockersEffectNetwork2018,
	title = {Super-blockers and the effect of network structure on information cascades},
	url = {http://arxiv.org/abs/1802.05039},
	abstract = {Modelling information cascades over online social networks is important in fields from marketing to civil unrest prediction, however the underlying network structure strongly affects the probability and nature of such cascades. Even with simple cascade dynamics the probability of large cascades are almost entirely dictated by network properties, with well-known networks such as Erdos-Renyi and Barabasi-Albert producing wildly different cascades from the same model. Indeed, the notion of 'superspreaders' has arisen to describe highly influential nodes promoting global cascades in a social network. Here we use a simple model of global cascades to show that the presence of locality in the network increases the probability of a global cascade due to the increased vulnerability of connecting nodes. Rather than 'super-spreaders', we find that the presence of these highly connected 'super-blockers' in heavy-tailed networks in fact reduces the probability of global cascades, while promoting information spread when targeted as the initial spreader.},
	urldate = {2018-05-03},
	journal = {arXiv:1802.05039 [physics]},
	author = {Gray, Caitlin and Mitchell, Lewis and Roughan, Matthew},
	month = feb,
	year = {2018},
	note = {arXiv: 1802.05039},
	keywords = {Computer Science - Social and Information Networks, Physics - Physics and Society, Physics - Data Analysis, Statistics and Probability},
	file = {arXiv\:1802.05039 PDF:C\:\\Users\\au687269\\Zotero\\storage\\H396K764\\Gray et al. - 2018 - Super-blockers and the effect of network structure.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\HV22RYJ7\\1802.html:text/html},
}

