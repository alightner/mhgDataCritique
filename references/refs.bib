	issn = {0010-0277},
	shorttitle = {Epistemic justifications for belief in the unobservable},
	url = {http://www.sciencedirect.com/science/article/pii/S0010027720300925},
	doi = {10.1016/j.cognition.2020.104273},
	abstract = {Children hold beliefs about religious and scientific entities, such as angels or germs, that they cannot directly observe or interact with. Given their limited opportunities for first-hand observation, children's beliefs in these entities are a clear example of cultural learning and are likely to vary based on cultural factors. In the present study, we investigated variation in the epistemic stance of 4–11-year-old children growing up in a religious minority in China (N = 47), a religious majority in Iran (N = 85), and a religious majority in the U.S. (N = 74). To assess the role of community status as a domain-specific, as opposed to a domain-general, factor contributing to children's beliefs about unobservable entities, we compared children's beliefs about religious unobservable entities with their beliefs about scientific unobservable entities in these three communities. In all three communities, younger and older children were confident that unobservable religious and scientific entities exist. However, compared to children in Iran and the U.S., children from the religious minority group in China were more likely to justify their ontological beliefs about religious entities by appealing to the source of their beliefs. These results highlight the impact of community status on learning from testimony about unobservable entities. Additionally, the results show that under certain circumstances – notably when holding minority beliefs – tracking the source of beliefs serves as a central epistemic justification.},
	language = {en},
	urldate = {2020-04-10},
	journal = {Cognition},
	author = {Davoodi, Telli and Cui, Kelly Yixin and Clegg, Jennifer M. and Yan, Fang E. and Payir, Ayse and Harris, Paul L. and Corriveau, Kathleen H.},
	month = apr,
	year = {2020},
	keywords = {Belief justification, Community status, Religious entities, Scientific entities, Source tracking, Unobservable phenomena},
	pages = {104273},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\JFZI6R3Y\\Davoodi et al. - 2020 - Epistemic justifications for belief in the unobser.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\3TL73PT3\\S0010027720300925.html:text/html},
