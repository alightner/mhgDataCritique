	keywords = {Evolution, Psychology, Social Psychology, cultural evolution, social learning, Anthropology, Sociology, Societies, Social and Behavioral Sciences, Developmental Psychology, and Cultures, Applied Mathematics, Arts and Humanities, Biology, Cognition and Perception, cultural selection, cultural systems, Ecology and Evolutionary Biology, Life Sciences, Other Languages, Other Social and Behavioral Sciences, Physical Sciences and Mathematics, Social and Cultural Anthropology, Sociology of Culture},
