@article{fosterGeneralModelEvolution2006,
	title = {A general model for the evolution of mutualisms},
	volume = {19},
	issn = {1420-9101},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1420-9101.2005.01073.x},
	doi = {10.1111/j.1420-9101.2005.01073.x},
	abstract = {The evolution of mutualisms presents a puzzle. Why does selection favour cooperation among species rather than cheaters that accept benefits but provide nothing in return? Here we present a general model that predicts three key factors will be important in mutualism evolution: (i) high benefit to cost ratio, (ii) high within-species relatedness and (iii) high between-species fidelity. These factors operate by moderating three types of feedback benefit from mutualism: cooperator association, partner-fidelity feedback and partner choice. In defining the relationship between these processes, our model also allows an assessment of their relative importance. Importantly, the model suggests that phenotypic feedbacks (partner-fidelity feedback, partner choice) are a more important explanation for between-species cooperation than the development of genetic correlations among species (cooperator association). We explain the relationship of our model to existing theories and discuss the empirical evidence for our predictions.},
	language = {en},
	number = {4},
	urldate = {2019-01-20},
	journal = {Journal of Evolutionary Biology},
	author = {Foster, K. R. and Wenseleers, T.},
	year = {2006},
	keywords = {cooperation, mutualism, biofilms, conflict, Hamilton's rule, vertical transmission},
	pages = {1283--1293},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\Z8X5HLKB\\Foster and Wenseleers - 2006 - A general model for the evolution of mutualisms.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\KGW562VS\\j.1420-9101.2005.01073.html:text/html},
