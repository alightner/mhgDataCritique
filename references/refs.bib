	author = {Bouhlel, Imen and Wu, Charley M. and Ilanaki, Nobuyuki and Goldstone, Robert L.},
	month = feb,
	year = {2018},
	note = {Publisher: Cold Spring Harbor Laboratory
Section: New Results},
	pages = {258715},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\TF9Z4NA5\\Bouhlel et al. - 2018 - Sharing is not erring Pseudo-reciprocity in colle.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\7E2ISMLI\\258715v1.html:text/html},
