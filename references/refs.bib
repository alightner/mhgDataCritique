	pages = {20141709},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\IJ5QYRFB\\Heyes and Pearce - 2015 - Not-so-social learning strategies.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\ET588Q9D\\rspb.2014.html:text/html},
