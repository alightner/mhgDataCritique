	pmid = {33127754},
	note = {Publisher: National Academy of Sciences
Section: Social Sciences},
	keywords = {cooperation, trust, GDP, political revolution, text mining},
	file = {Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\FXDPU3WM\\2009571117.html:text/html;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\3DEE65QI\\2009571117.html:text/html},
