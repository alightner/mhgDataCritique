@techreport{herbstAbstractingTimeMemory2021,
	title = {Abstracting time in memory},
	url = {https://psyarxiv.com/478cx/},
	abstract = {Planning the future relies on the ability to remember how long events last, yet, how durations are stored in memory is unknown. Here, we developed a novel n-item delayed duration reproduction task to assess whether elapsed time is stored as a continuous feature or as an abstract item in memory. In three experiments (N = 58), participants listened to non-rhythmic sequences composed of empty time intervals (durations), which they had to reproduce as precisely as possible following a delay period. We manipulated the number of time intervals (n-item) and the overall sequence duration to separate their effects on recall precision. The precision of temporal reproduction systematically decreased with an increasing number of items. Our results suggest that the number of time intervals, not their duration, determines recall precision. We interpret this as evidence towards an abstract representation of duration in working memory.},
	urldate = {2021-05-10},
	institution = {PsyArXiv},
	author = {Herbst, Sophie and Mangione, Izem and Kononowicz, Tadeusz and Wassenhove, Virginie van},
