	shorttitle = {Cooperation in an {Uncertain} {World}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4937096/},
	doi = {10.1007/s10745-016-9823-z},
	abstract = {Using an agent-based model to study risk-pooling in herder dyads using rules derived from Maasai osotua (“umbilical cord”) relationships, Aktipis et al. () found that osotua transfers led to more risk-pooling and better herd survival than both no transfers and transfers that occurred at frequencies tied to those seen in the osotua simulations. Here we expand this approach by comparing osotua-style transfers to another type of livestock transfer among Maasai known as esile (“debt”). In osotua, one asks if in need, and one gives in response to such requests if doing so will not threaten one’s own survival. In esile relationships, accounts are kept and debts must be repaid. We refer to these as “need-based” and “account-keeping” systems, respectively. Need-based transfers lead to more risk pooling and higher survival than account keeping. Need-based transfers also lead to greater wealth equality and are game theoretically dominant to account-keeping rules.},
	urldate = {2018-03-28},
	journal = {Human Ecology},
	author = {Aktipis, Athena and de Aguiar, Rolando and Flaherty, Anna and Iyer, Padmini and Sonkoi, Dennis and Cronk, Lee},
	year = {2016},
	pmid = {27445430},
	pmcid = {PMC4937096},
	pages = {353--364},
	file = {PubMed Central Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\K99SMSFG\\Aktipis et al. - 2016 - Cooperation in an Uncertain World For the Maasai .pdf:application/pdf},
}

