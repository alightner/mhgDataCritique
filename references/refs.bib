	journal = {Nature},
	author = {González-Forero, Mauricio and Gardner, Andy},
	month = may,
	year = {2018},
	note = {Number: 7706
Publisher: Nature Publishing Group},
	pages = {554--557},
	file = {Accepted Version:C\:\\Users\\au687269\\Zotero\\storage\\H4YMLSQW\\González-Forero and Gardner - 2018 - Inference of ecological and social drivers of huma.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\SWBTULQE\\s41586-018-0127-x.html:text/html;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\BT9SBE6U\\s41586-018-0127-x.html:text/html},
