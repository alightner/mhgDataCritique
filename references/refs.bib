	file = {ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\2TWZ5C4Z\\Kemp et al. - 2010 - A probabilistic model of theory formation.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\SW6QZD2S\\Kemp et al. - 2010 - A probabilistic model of theory formation.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\MNTAH8XS\\S0010027709002169.html:text/html;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\RU5ZZST5\\S0010027709002169.html:text/html},
