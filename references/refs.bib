	pages = {233--240},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\ZWU4MTSP\\Sánchez and Cuesta - 2005 - Altruism may arise from individual selection.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\IZXKDXXI\\S0022519305000226.html:text/html;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\4R2TDFAW\\S0022519305000226.html:text/html;ScienceDirect Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\ADKU259M\\Sánchez and Cuesta - 2005 - Altruism may arise from individual selection.html:text/html},
