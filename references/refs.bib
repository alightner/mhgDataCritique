	year = {2012},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1551-6709.2012.01242.x},
	keywords = {Theory of mind, Supernatural punishment, Cognitive science of religion, Socially strategic information, Supernatural agents},
	pages = {846--869},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\E2L29NU7\\Purzycki et al. - 2012 - What Does God Know Supernatural Agents’ Access to.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\LKVVMLA2\\j.1551-6709.2012.01242.html:text/html},
