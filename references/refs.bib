	month = jul,
	year = {2016},
	keywords = {dimensional reduction, feature vector, neuroinformatics, predictive modeling, reverse correlation, systems identification, time series},
	pages = {221--259},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\HMAJDV35\\Aljadeff et al. - 2016 - Analysis of Neuronal Spike Trains, Deconstructed.pdf:application/pdf},
}

