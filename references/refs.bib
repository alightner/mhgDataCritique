	note = {\_eprint: https://anthrosource.onlinelibrary.wiley.com/doi/pdf/10.1111/aman.13485},
	file = {Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\JRYQS67H\\aman.html:text/html},
}

@article{pageReconstructingPrehistoricDemography,
	title = {Reconstructing prehistoric demography: {What} role for extant hunter-gatherers?},
	volume = {n/a},
	copyright = {© 2020 The Authors. Evolutionary Anthropology published by Wiley Periodicals LLC.},
	issn = {1520-6505},
