	year = {2006},
	pmid = {16594767},
	keywords = {Humans, Analysis of Variance, Child, Models, Statistical, Reproducibility of Results, Bias, Data Interpretation, Statistical, Dyslexia, Least-Squares Analysis, Likelihood Functions, Linear Models, Normal Distribution, Regression Analysis},
	pages = {54--71},
}

