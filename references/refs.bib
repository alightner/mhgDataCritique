@article{agrenSelfishGeneticElements2018,
	title = {Selfish genetic elements},
	volume = {14},
	issn = {1553-7404},
	url = {https://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1007700},
	doi = {10.1371/journal.pgen.1007700},
	abstract = {Selfish genetic elements (historically also referred to as selfish genes, ultra-selfish genes, selfish DNA, parasitic DNA, genomic outlaws) are genetic segments that can enhance their own transmission at the expense of other genes in the genome, even if this has no or a negative effect on organismal fitness. [1–6] Genomes have traditionally been viewed as cohesive units, with genes acting together to improve the fitness of the organism. However, when genes have some control over their own transmission, the rules can change, and so just like all social groups, genomes are vulnerable to selfish behaviour by their parts. Early observations of selfish genetic elements were made almost a century ago, but the topic did not get widespread attention until several decades later. Inspired by the gene-centred views of evolution popularized by George Williams[7] and Richard Dawkins,[8] two papers were published back-to-back in Nature in 1980—by Leslie Orgel and Francis Crick[9] and Ford Doolittle and Carmen Sapienza[10] respectively—introducing the concept of selfish genetic elements (at the time called “selfish DNA”) to the wider scientific community. Both papers emphasized that genes can spread in a population regardless of their effect on organismal fitness as long as they have a transmission advantage. Selfish genetic elements have now been described in most groups of organisms, and they demonstrate a remarkable diversity in the ways by which they promote their own transmission.[11] Though long dismissed as genetic curiosities, with little relevance for evolution, they are now recognized to affect a wide swath of biological processes, ranging from genome size and architecture to speciation.[12]},
	language = {en},
	number = {11},
	urldate = {2019-01-20},
	journal = {PLOS Genetics},
	author = {Ågren, J. Arvid and Clark, Andrew G.},
	month = nov,
	year = {2018},
	keywords = {Evolutionary genetics, Mitochondria, Genetic elements, Genome evolution, Genomic imprinting, Invertebrate genomics, Plant genomics, Transposable elements},
	pages = {e1007700},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\RGZMDUAF\\Ågren and Clark - 2018 - Selfish genetic elements.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\YDLU5DSL\\article.html:text/html},
}

