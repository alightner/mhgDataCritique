Section: Physical Sciences},
	keywords = {A Song of Ice and Fire, comparative literature, Dunbar’s number, Game of Thrones, networks},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\5UAKS8SP\\Gessey-Jones et al. - 2020 - Narrative structure of A Song of Ice and Fire crea.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\4IS694FZ\\2006465117.html:text/html;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\9UK4SFYK\\2006465117.html:text/html},
