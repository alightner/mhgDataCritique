	journal = {Cognitive Science},
	author = {Lerique, Sébastien and Roth, Camille},
	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/cogs.12494},
	keywords = {Cultural evolution, Cultural attraction, Semantic network, Big data, Data mining, Psycholinguistics, Recollection bias, Word recall},
	pages = {188--219},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\TNMDUTGV\\Lerique and Roth - 2018 - The Semantic Drift of Quotations in Blogspace A C.pdf:application/pdf},
