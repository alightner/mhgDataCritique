@techreport{webbNormalizationConsumerValuations2019,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {The {Normalization} of {Consumer} {Valuations}: {Context}-{Dependent} {Preferences} from {Neurobiological} {Constraints}},
