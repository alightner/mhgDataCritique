@article{conradsStrategicIgnoranceUltimatum2013,
	title = {Strategic ignorance in ultimatum bargaining},
	volume = {92},
	issn = {0167-2681},
	url = {https://econpapers.repec.org/article/eeejeborg/v_3a92_3ay_3a2013_3ai_3ac_3ap_3a104-115.htm},
	abstract = {In his classic article “An Essay on Bargaining” Schelling (1956) argues that ignorance might actually be strength rather than weakness. We test and confirm Schelling's conjecture in a simple take-it-or-leave-it bargaining experiment where the proposer can choose between two possible offers. Option A always gives the proposer a higher payoff than option B. The payoff of the responder depends on the (randomly determined) state of nature. In one state payoffs of the two players are aligned whereas they are not aligned in the other state. The responder is always informed about the actual state. The proposer knows the actual state in our first treatment but not in the second. We find that proposers indeed benefit from ignorance because the responders accept almost all offers (even the unfavorable ones) if the payoffs are not transparent for the proposer. In additional treatments we investigate bargaining situations where the proposer can deliberately remain ignorant.},
	number = {C},
	journal = {Journal of Economic Behavior \& Organization},
	author = {Conrads, Julian and Irlenbusch, Bernd},
	year = {2013},
	keywords = {intentions, Strategic ignorance, Bargaining, Experiment},
	pages = {104--115},
	file = {RePEc Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\JS63V98P\\v_3a92_3ay_3a2013_3ai_3ac_3ap_3a104-115.html:text/html},
}

