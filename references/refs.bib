\_eprint: https://doi.org/10.1080/2153599X.2017.1378709},
	keywords = {cognitive science of religion, HADD, perception, Agency Detection, predictive processing, religious teachings, virtual reality},
	pages = {52--64},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\8ZR2H9UG\\Andersen et al. - 2019 - Agency detection in predictive minds a virtual re.pdf:application/pdf},
}

@article{snareyNaturalEnvironmentImpact1996,
	title = {The {Natural} {Environment}'s {Impact} upon {Religious} {Ethics}: {A} {Cross}-{Cultural} {Study}},
