	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\PPSUPHKS\\Quervain et al. - 2004 - The Neural Basis of Altruistic Punishment.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\R983SECA\\1254.html:text/html},
