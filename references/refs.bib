	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\ENWKYQVI\\Buskell - Cumulative culture and complex cultural traditions.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\XVP62BQI\\mila.html:text/html},
}

@article{enquistModellingEvolutionDiversity2011b,
	title = {Modelling the evolution and diversity of cumulative culture},
