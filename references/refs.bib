@article{haoNodeSurvivalNetworks2015,
	title = {Node {Survival} in {Networks} under {Correlated} {Attacks}},
	volume = {10},
	issn = {1932-6203},
	url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0125467},
	doi = {10.1371/journal.pone.0125467},
	abstract = {We study the interplay between correlations, dynamics, and networks for repeated attacks on a socio-economic network. As a model system we consider an insurance scheme against disasters that randomly hit nodes, where a node in need receives support from its network neighbors. The model is motivated by gift giving among the Maasai called Osotua. Survival of nodes under different disaster scenarios (uncorrelated, spatially, temporally and spatio-temporally correlated) and for different network architectures are studied with agent-based numerical simulations. We find that the survival rate of a node depends dramatically on the type of correlation of the disasters: Spatially and spatio-temporally correlated disasters increase the survival rate; purely temporally correlated disasters decrease it. The type of correlation also leads to strong inequality among the surviving nodes. We introduce the concept of disaster masking to explain some of the results of our simulations. We also analyze the subsets of the networks that were activated to provide support after fifty years of random disasters. They show qualitative differences for the different disaster scenarios measured by path length, degree, clustering coefficient, and number of cycles.},
	language = {en},
	number = {5},
	urldate = {2019-01-09},
	journal = {PLOS ONE},
	author = {Hao, Yan and Armbruster, Dieter and Hütt, Marc-Thorsten},
	month = may,
	year = {2015},
	keywords = {Probability distribution, Cattle, Social networks, Livestock, Agent-based modeling, Ecological niches, Insurance, Social systems},
	pages = {e0125467},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\S8GRC5H5\\Hao et al. - 2015 - Node Survival in Networks under Correlated Attacks.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\YYM8SBI7\\article.html:text/html},
