	pages = {20132864},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\5TV2PQIL\\Toelch et al. - 2014 - Individual consistency and flexibility in human so.pdf:application/pdf},
}

@article{mesoudiHigherFrequencySocial2015b,
	title = {Higher frequency of social learning in {China} than in the {West} shows cultural variation in the dynamics of cultural evolution},
