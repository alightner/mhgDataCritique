	keywords = {cooperation, game, social behavior, stochastic dynamics},
	pages = {8601--8604},
	file = {Full Text PDF:C\:\\Users\\au687269\\Zotero\\storage\\2BQ69KSU\\Tarnita et al. - 2009 - Evolutionary dynamics in set structured population.pdf:application/pdf;Snapshot:C\:\\Users\\au687269\\Zotero\\storage\\MS77EG7E\\8601.html:text/html},
}

@article{allenEvolutionaryDynamicsAny2017,
